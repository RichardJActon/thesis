file = {:home/richardjamesacton/Documents/PhD{\_}Stuff/Papers/mendeleyPapers/Leitersdorf, Chakravarti, Hobbs{\_}1989{\_}American journal of human genetics{\_}Polymorphic DNA haplotypes at the LDL receptor locus.pdf:pdf},
