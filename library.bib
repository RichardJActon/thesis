keywords = {DNA methylation,Werner syndrome,epigenetic clock,epigenetics,progeria},
month = {apr},
number = {4},
pages = {1143--1152},
pmid = {28377537},
title = {{Accelerated epigenetic aging in Werner syndrome.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/28377537},
volume = {9},
year = {2017}
}
