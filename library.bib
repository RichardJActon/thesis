isbn = {0890-9369 (Print)$\backslash$n0890-9369 (Linking)},
issn = {0890-9369},
journal = {Genes {\&} development},
keywords = {Chromatin,DNMT1,G9a,Methylation,Replication,SUV39H1},
month = {nov},
