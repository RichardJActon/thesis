}
@article{Weber2014,
abstract = {Eukaryotic gene regulation involves a balance between packaging of the genome into nucleosomes and enabling access to regulatory proteins and RNA polymerase. Nucleosomes are integral components of gene regulation that restrict access to both regulatory sequences and the underlying template. Whereas canonical histones package the newly replicated genome, they can be replaced with histone variants that alter nucleosome structure, stability, dynamics, and, ultimately, DNA accessibility. Here we consider how histone variants and their interacting partners are involved in transcriptional regulation through the creation of unique chromatin states.},
