doi = {10.1038/nature20784},
file = {:home/richardjamesacton/Documents/PhD{\_}Stuff/Papers/mendeleyPapers/Wahl et al.{\_}2017{\_}Nature{\_}Epigenome-wide association study of body mass index, and the adverse outcomes of adiposity.pdf:pdf},
