editor = {Camilli, Andrew},
file = {:home/richardjamesacton/Documents/PhD{\_}Stuff/Papers/mendeleyPapers/Hobley et al.{\_}2012{\_}PLoS Pathogens{\_}Discrete Cyclic di-GMP-Dependent Control of Bacterial Predation versus Axenic Growth in Bdellovibrio b.pdf:pdf},
