file = {:home/richardjamesacton/Documents/PhD{\_}Stuff/Papers/mendeleyPapers/Ko et al.{\_}2010{\_}Journal of molecular biology{\_}Structure of PP4397 reveals the molecular basis for different c-di-GMP binding modes by Pilz.pdf:pdf},
