file = {:home/richardjamesacton/Documents/PhD{\_}Stuff/Papers/mendeleyPapers/Keam et al.{\_}2014{\_}Nucleic Acids Research{\_}The human Piwi protein Hiwi2 associates with tRNA-derived piRNAs in somatic cells.pdf:pdf},
issn = {0305-1048},
journal = {Nucleic Acids Research},
month = {aug},
number = {14},
pages = {8984--8995},
title = {{The human Piwi protein Hiwi2 associates with tRNA-derived piRNAs in somatic cells}},
url = {https://academic.oup.com/nar/article-lookup/doi/10.1093/nar/gku620},
volume = {42},
year = {2014}
}
