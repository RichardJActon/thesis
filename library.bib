doi = {10.1016/j.cmet.2018.04.013},
file = {:home/richardjamesacton/Documents/PhD{\_}Stuff/Papers/mendeleyPapers/Lu et al.{\_}2018{\_}Cell Metabolism{\_}The Polycomb-Dependent Epigenome Controls $\beta$ Cell Dysfunction, Dedifferentiation, and Diabetes.pdf:pdf},
