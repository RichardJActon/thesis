file = {:home/richardjamesacton/Documents/PhD{\_}Stuff/Papers/mendeleyPapers/Goldberg et al.{\_}2010{\_}Aging{\_}Chemical genetic screen identifies lithocholic acid as an anti-aging compound that extends yeast chronologica.pdf:pdf},
isbn = {1945-4589},
issn = {1945-4589},
journal = {Aging},
keywords = {Anti-aging compounds,Caloric restriction,Chemical biology,Longevity,Yeast,cellular aging},
month = {jul},
number = {7},
pages = {393--414},
pmid = {20622262},
title = {{Chemical genetic screen identifies lithocholic acid as an anti-aging compound that extends yeast chronological life span in a TOR-independent manner, by modulating housekeeping longevity assurance processes}},
url = {http://www.aging-us.com/article/100168},
volume = {2},
