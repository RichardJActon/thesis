issn = {10478477},
journal = {Journal of Structural Biology},
keywords = {Algorithms,Animals,Artifacts,Artificial Intelligence,Bdellovibrio,Bdellovibrio: cytology,Computer-Assisted,Computer-Assisted: methods,Computer-Assisted: standards,Electron Microscope Tomography,Electron Microscope Tomography: methods,Electron Microscope Tomography: standards,HIV Infections,HIV Infections: pathology,Humans,Image Processing,Macrophages,Macrophages: pathology,Macrophages: virology},
