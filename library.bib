issn = {0261-4189},
journal = {The EMBO journal},
month = {dec},
number = {24},
pages = {7219--30},
pmid = {9405351},
title = {{The 1.25 A crystal structure of sepiapterin reductase reveals its binding mode to pterins and brain neurotransmitters.}},
