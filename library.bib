isbn = {0047-6374 (Print)$\backslash$r0047-6374 (Linking)},
issn = {00476374},
journal = {Mechanisms of Ageing and Development},
keywords = {Aging,DNA methylation,RLGS,T cells},
mendeley-tags = {RLGS},
month = {sep},
number = {11},
pages = {1487--1503},
pmid = {12425956},
title = {{Infrequent occurrence of age-dependent changes in CpG island methylation as detected by restriction landmark genome scanning}},
