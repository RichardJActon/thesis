journal = {Nucleic Acids Research},
keywords = {Catalytic Domain,Dioxygenases,Dioxygenases: chemistry,Dioxygenases: metabolism,Humans,Membrane Proteins,Membrane Proteins: chemistry,Membrane Proteins: metabolism,Models,Molecular,Protein Conformation,RNA,RNA: metabolism,Static Electricity},
