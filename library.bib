keywords = {Cell migration,Extracellular matrix,Integrin,Invasion,Melanoma,Metastasis,RNA polymerase III (Pol III),TRNA repertoire,TRNAiMet},
number = {10},
pages = {1371--1379},
title = {{The initiator methionine tRNA drives cell migration and invasion leading to increased metastatic potential in melanoma}},
