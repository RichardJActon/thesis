issn = {1079-5006},
journal = {The journals of gerontology. Series A, Biological sciences and medical sciences},
keywords = {80 and over,Adolescent,Adult,Aged,Alu Elements,Alu Elements: genetics,Female,Genetic,Genotype,Humans,Introns,Longevity,Longevity: genetics,Lymphocytes,Lymphocytes: metabolism,Male,Messenger,Messenger: genetics,Middle Aged,Polymorphism,RNA,Reverse Transcriptase Polymerase Chain Reaction},
month = {jun},
number = {6},
pages = {547--56},
pmid = {16799135},
title = {{A polymorphism of the YTHDF2 gene (1p35) located in an Alu-rich genomic domain is associated with human longevity.}},
