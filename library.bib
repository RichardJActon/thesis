issn = {1559-2294},
journal = {Epigenetics},
keywords = {5-hmc,5-hydroxymethyl-cytosine,5-hydroxymethyl-cytosine distribution,5-methyl-cytosine,a considerable attention,according to several reports,due to its potential,epigenetics,evolution,is a form of,modified cytosine,regeneration,role in transcriptional regulation,semi-quantitative immunohistochemistry,which has recently attracted},
