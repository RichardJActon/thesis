issn = {0022-0957},
journal = {Journal of Experimental Botany},
keywords = {Arabidopsis,Arabidopsis: growth {\&} development,Arabidopsis: metabolism,Gene Expression Regulation,Plant,Plant Growth Regulators,Plant Growth Regulators: metabolism,Plant: genetics,Plant: physiology,Salicylic Acid,Salicylic Acid: metabolism},
