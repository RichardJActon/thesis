@article{Crispatzu2017,
abstract = {Next-generation sequencing (NGS) has turned from a new and experimental technology into a standard procedure for cancer genome studies and clinical investigation. While a multitude of software packages for cancer genome data analysis have been made available, these need to be combined into efficient analytical workflows that cover multiple aspects relevant to a clinical environment and that deliver handy results within a reasonable time frame. Here, we introduce QuickNGS Cancer as a new suite of bioinformatics pipelines that is focused on cancer genomics and significantly reduces the analytical hurdles that still limit a broader applicability of NGS technology, particularly to clinically driven research. QuickNGS Cancer allows a highly efficient analysis of a broad variety of NGS data types, specifically considering cancer-specific issues, such as biases introduced by tumor impurity and aneuploidy or the assessment of genomic variations regarding their biomedical relevance. It delivers highly reproducible analysis results ready for interpretation within only a few days after sequencing, as shown by a reanalysis of 140 tumor/normal pairs from The Cancer Genome Atlas (TCGA) in which QuickNGS Cancer detected a significant number of mutations in key cancer genes missed by a well-established mutation calling pipeline. Finally, QuickNGS Cancer obtained several unexpected mutations in leukemias that could be confirmed by Sanger sequencing.},
author = {Crispatzu, Giuliano and Kulkarni, Pranav and Toliat, Mohammad R. and N{\"{u}}rnberg, Peter and Herling, Marco and Herling, Carmen D. and Frommolt, Peter},
doi = {10.1002/humu.23275},
file = {:home/richardjamesacton/Documents/PhD{\_}Stuff/Papers/crispatzu2017{\_}({\_}{\_}QuickNGS-cancer{\_}{\_}).pdf:pdf},
issn = {10981004},
journal = {Human Mutation},
keywords = {analysis pipeline,cancer genomics,medical bioinformatics,next-generation sequencing},
number = {10},
pages = {1325--1335},
title = {{Semi-automated cancer genome analysis using high-performance computing}},
volume = {38},
year = {2017}
}
