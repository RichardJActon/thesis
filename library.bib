file = {:home/richardjamesacton/Documents/PhD{\_}Stuff/Papers/mendeleyPapers/Sugawara, Nikaido{\_}2014{\_}Antimicrobial agents and chemotherapy{\_}Properties of AdeABC and AdeIJK efflux systems of Acinetobacter baumannii c.pdf:pdf},
isbn = {9788578110796},
issn = {1098-6596},
