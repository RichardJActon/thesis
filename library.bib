issn = {0016-6731},
journal = {Genetics},
month = {jan},
number = {1},
pages = {75--86},
pmid = {8608934},
title = {{A mutation in the age-1 gene in Caenorhabditis elegans lengthens life and reduces hermaphrodite fertility.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/8608934},
volume = {118},
