keywords = {Animals,Antisense,Antisense: administration {\&} dosage,Antisense: chemistry,Antisense: genetics,Duchenne,Duchenne: genetics,Duchenne: pathology,Duchenne: therapy,Dystrophin,Dystrophin: genetics,Exons,Exons: genetics,Gene Expression,Genetic Therapy,Genetic Therapy: methods,Inbred mdx,Injections,Intraperitoneal,Mice,Morpholines,Morpholines: chemistry,Morpholinos,Muscles,Muscles: metabolism,Muscles: pathology,Muscular Dystrophy,Mutation,Oligonucleotides,Reverse Transcriptase Polymerase Chain Reaction},
month = {sep},
number = {9},
pages = {1587--1592},
pmid = {17579573},
title = {{Morpholino Oligomer–Mediated Exon Skipping Averts the Onset of Dystrophic Pathology in the mdx Mouse}},
