}
@article{Ambrosi2017,
abstract = {DNA methylation is one of the most extensively studied epigenetic marks. It is involved in transcriptional gene silencing, it plays important roles during mammalian development and its perturbation is often associated with human diseases. In mammalian genomes, DNA methylation is a prevalent modification that decorates the majority of cytosines. It is found at the promoters and enhancers of inactive genes, at repetitive elements and within transcribed gene bodies. Its presence at promoters is dynamically linked to gene activity, suggesting that it could directly influence gene expression patterns and cellular identity. The genome-wide distribution and dynamic behaviour of this mark has been studied to great detail in a variety of tissues and cell lines, including early embryonic development and in embryonic stem cells. In combination with functional studies, these genome-wide maps of DNA methylation revealed interesting features of this mark and provided important insights into its dynamic nature and potential functional role in genome regulation. In this review we discuss how these recent observations, in combination with insights obtained from biochemical and functional genetics studies, have expanded our current knowledge about the regulation and context-dependent roles of DNA methylation in mammalian genomes.},
author = {Ambrosi, Christina and Manzo, Massimiliano and Baubec, Tuncay},
doi = {10.1016/j.jmb.2017.02.008},
file = {:home/richardjamesacton/Documents/PhD{\_}Stuff/Papers/mendeleyPapers/Ambrosi, Manzo, Baubec{\_}2017{\_}Journal of Molecular Biology{\_}Dynamics and Context-Dependent Roles of DNA Methylation.pdf:pdf},
