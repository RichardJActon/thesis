isbn = {1305901508013},
issn = {1474-760X},
journal = {Genome biology},
keywords = {2,and,associated protein 1,centrosome and spindle pole,circrna,circular rna,cortical development,cspp1,embryonic,fetal development,hdac2,histone deacetylase 2,in situ hybridization,ish,ndfip2,nedd4 family interacting protein,pig brain,regulating,rim2,rims2,sus scrofa,synaptic membrane exocytosis 2,tlk1,tmeff1,tousled-like kinase 1,transmembrane protein with egf-like,two follistatin-like domains 1},
