issn = {15681637},
journal = {Ageing Research Reviews},
keywords = {Aging,Alzheimer's,Atherosclerosis,Diabetes,Hypertension,Parkinson's},
month = {jan},
number = {November 2018},
pages = {11--26},
publisher = {Elsevier},
title = {{Age-related diseases as vicious cycles}},
