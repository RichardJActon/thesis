@article{Kang2007a,
abstract = {In general, methylation of the promoter regions is inversely correlated with gene expression. The transitional CpG area between the promoter-associated CpG islands and the nearby retroelements is often methylated in a tissue-specific manner. This study analyzed the relationship between gene expression and the methylation of the transitional CpGs in two human stromal cells derived from the bone marrow (BMSC) and adipose tissue (ATSC), both of which have a multilineage differentiation potential. The transitional CpGs of the osteoblast-specific (RUNX2 and BGLAP), adipocyte-specific (PPARgamma2), housekeeping (CDKN2A and MLH1), and mesenchyme-unrelated (RUNX3) genes were examined by methylation-specific PCR. The expression of each gene was measured using reverse-transcription PCR analysis. The RUNX2, BGLAP, and CDKN2A genes in the BMSC, and the PPARgamma2 gene in the ATSC exhibited hypomethylation of the transitional CpGs along with the strong expression. The CpG island of RUNX3 gene not expressed in both BMSC and ATSC was hypermethylated. Transitional hypomethylation of the MLH1 gene was accompanied by the higher expression in the BMSC than in the ATSC. The weakly methylated CpGs of the PPARgamma2 gene in the BMSC became hypomethylated along with the strong expression during the osteoblastic differentiation. There were no notable changes in the transitional methylation and expression of the genes other than PPARgamma2 after the differentiation. Therefore, the transitional methylation and gene expression established in mesenchymal cells tend to be consistently preserved under the induction of differentiation. Weak transitional methylation of the PPARgamma2 gene in the BMSC suggests a methylation-dependent mechanism underlying the adiopogenesis of bone marrow.},
author = {Kang, Moo-Il and Kim, Hye-Soo and Jung, Yu-Chae and Kim, Young-Ho and Hong, Seung-Jin and Kim, Mi-Kyoung and Baek, Ki-Hyun and Kim, Chun–Choo and Rhyu, Mun-Gan},
doi = {10.1002/jcb.21291},
file = {:home/richardjamesacton/Documents/PhD{\_}Stuff/Papers/mendeleyPapers/Kang et al.{\_}2007{\_}Journal of Cellular Biochemistry{\_}Transitional CpG methylation between promoters and retroelements of tissue-specific ge.pdf:pdf},
isbn = {0730-2312 (Print)$\backslash$r0730-2312 (Linking)},
issn = {07302312},
journal = {Journal of Cellular Biochemistry},
keywords = {Cell differentiation,Mesenchymal cell,Methylation,mRNA expression},
month = {sep},
number = {1},
pages = {224--239},
pmid = {17352407},
title = {{Transitional CpG methylation between promoters and retroelements of tissue-specific genes during human mesenchymal cell differentiation}},
url = {http://doi.wiley.com/10.1002/jcb.21291},
volume = {102},
year = {2007}
}
@article{Cocozza2013,
abstract = {BACKGROUND: CpG dinucleotide-rich genomic DNA regions, known as CpG islands (CGIs), can be methylated at their cytosine residues as an epigenetic mark that is stably inherited during cell mitosis. Differentially methylated regions (DMRs) are genomic regions showing different degrees of DNA methylation in multiple samples. In this study, we focused our attention on CGIs showing different DNA methylation between two culture replicas of the same cell line.$\backslash$n$\backslash$nRESULTS: We used methylation data of 35 cell lines from the Encyclopedia of DNA Elements (ENCODE) consortium to identify CpG islands that were differentially methylated between replicas of the same cell line and denoted them Inter Replicas Differentially Methylated CpG islands (IRDM-CGIs). We identified a group of IRDM-CGIs that was consistently shared by different cell lines, and denoted it common IRDM-CGIs. X chromosome CGIs were overrepresented among common IRDM-CGIs. Autosomal IRDM-CGIs were preferentially located in gene bodies and intergenic regions had a lower G + C content, a smaller mean length, and a reduced CpG percentage. Functional analysis of the genes associated with autosomal IRDM-CGIs showed that many of them are involved in DNA binding and development.$\backslash$n$\backslash$nCONCLUSIONS: Our results show that several specific functional and structural features characterize common IRDM-CGIs. They may represent a specific subset of CGIs that are more prone to being differentially methylated for their intrinsic characteristics.},
author = {Cocozza, Sergio and Scala, Giovanni and Miele, Gennaro and Castaldo, Imma and Monticelli, Antonella},
doi = {10.1186/1471-2164-14-692},
file = {:home/richardjamesacton/Documents/PhD{\_}Stuff/Papers/mendeleyPapers/Cocozza et al.{\_}2013{\_}BMC Genomics{\_}A distinct group of CpG islands shows differential DNA methylation between replicas of the same cell li.pdf:pdf},
issn = {1471-2164},
journal = {BMC Genomics},
keywords = {Base Composition,Base Composition: genetics,Cell Line,Chromosomes,CpG Islands,CpG Islands: genetics,DNA,DNA Methylation,DNA Methylation: genetics,Human,Human: genetics,Humans,Intergenic,Intergenic: genetics,X,X: genetics},
number = {1},
pages = {692},
pmid = {24106769},
title = {{A distinct group of CpG islands shows differential DNA methylation between replicas of the same cell line in vitro}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=4008136{\&}tool=pmcentrez{\&}rendertype=abstract http://bmcgenomics.biomedcentral.com/articles/10.1186/1471-2164-14-692},
volume = {14},
year = {2013}
}
@article{Liu2012,
abstract = {Bisulfite treatment of DNA followed by high-throughput sequencing (Bisulfite-seq) is an important method for studying DNA methylation and epigenetic gene regulation, yet current software tools do not adequately address single nucleotide polymorphisms (SNPs). Identifying SNPs is important for accurate quantification of methylation levels and for identification of allele-specific epigenetic events such as imprinting. We have developed a model-based bisulfite SNP caller, Bis-SNP, that results in substantially better SNP calls than existing methods, thereby improving methylation estimates. At an average 30× genomic coverage, Bis-SNP correctly identified 96{\%} of SNPs using the default high-stringency settings. The open-source package is available at http://epigenome.usc.edu/publicationdata/bissnp2011.},
