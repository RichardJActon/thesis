pmid = {26181258},
title = {{Correlation of Smoking-Associated DNA Methylation Changes in Buccal Cells With DNA Methylation Changes in Epithelial Cancer}},
url = {http://oncology.jamanetwork.com/article.aspx?doi=10.1001/jamaoncol.2015.1053},
