file = {:home/richardjamesacton/Documents/PhD{\_}Stuff/Papers/mendeleyPapers/Yoshida, Cai, Inouye{\_}2002{\_}Molecular microbiology{\_}Interaction of EnvZ, a sensory histidine kinase, with phosphorylated OmpR, the cognate.pdf:pdf},
