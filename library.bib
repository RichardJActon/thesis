issn = {1474-760X},
journal = {Genome biology},
keywords = {Alternative Splicing,Amino Acid,Amino Acid Sequence,Base Sequence,Cloning,Down Syndrome,Down Syndrome: genetics,Down Syndrome: metabolism,Endopeptidases,Endopeptidases: genetics,Endopeptidases: metabolism,Humans,Messenger,Messenger: metabolism,Molecular,Molecular Sequence Data,Protein Isoforms,Protein Isoforms: genetics,Protein Isoforms: metabolism,RNA,RNA Splice Sites,Sequence Homology,Tissue Distribution,Ubiquitin,Ubiquitin Thiolesterase,Ubiquitin: metabolism},
