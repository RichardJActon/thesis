journal = {Microbiology (Reading, England)},
keywords = {Amino Acid Substitution,Animals,Bacterial Proteins,Bacterial Proteins: physiology,Escherichia coli,Escherichia coli: growth {\&} development,Escherichia coli: pathogenicity,Female,Genetic Complementation Test,Humans,Hydrogen-Ion Concentration,Mice,Mutagenesis,Osmolar Concentration,Site-Directed,Trans-Activators,Trans-Activators: physiology,Urinary Tract,Urinary Tract Infections,Urinary Tract Infections: microbiology,Urinary Tract: microbiology,Virulence},
month = {jun},
