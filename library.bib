keywords = {Bacteria,Bacteria: genetics,Bacteria: metabolism,Bacterial,Bacterial Proteins,Bacterial Proteins: genetics,Bacterial Proteins: metabolism,Bacterial: genetics,Chlamydia,Chlamydia: genetics,Chlamydia: metabolism,Genome},
month = {jan},
