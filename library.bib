journal = {Journal of Epidemiology {\&} Community Health},
keywords = {Child,DNA Fingerprinting,DNA Fingerprinting: methods,Extramarital Relations,Family,Female,Genetic Techniques,Health Services Accessibility,Humans,Incidental Findings,Male,Paternity,Prevalence,Public Health,Risk Factors,Socioeconomic Factors,Tissue and Organ Procurement},
month = {sep},
