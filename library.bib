editor = {Hancock, John},
file = {:home/richardjamesacton/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Min et al. - 2018 - Meffil efficient normalization and analysis of very large DNA methylation datasets.pdf:pdf},
issn = {1367-4803},
journal = {Bioinformatics},
month = {jun},
