@article{Bulmer1987,
abstract = {The use of synonymous codons is strongly biased in the bacterium Escherichia coli and yeast, comprising both bias between codons recognized by the same transfer RNA and bias between groups of codons recognized by different synonymous tRNAs (reviewed in ref. 1). A major determinant of the second sort of bias is tRNA content, codons recognized by abundant tRNAs being used more often than those recognised by rare tRNAs, particularly in highly expressed genes, probably owing to selection at the level of translation against codons recognized by rare tRNAs1-3. Conversely, codon usage is likely to exert selection pressure on tRNA abundance4. Here I develop a model for the coevolution of codon usage and tRNA abundance which explains why there are unequal abundances of synonymous tRNAs leading to biased usage between groups of codons recognized by them in unicellular organisms. {\textcopyright} 1987 Nature Publishing Group.},
author = {Bulmer, Michael},
doi = {10.1038/325728a0},
file = {:home/richardjamesacton/Documents/PhD{\_}Stuff/Papers/Bulmer1987{\_}Nat({\_}{\_}coevolution-codon-usage-tRNA-abundance{\_}{\_}).pdf:pdf},
issn = {00280836},
journal = {Nature},
number = {6106},
pages = {728--730},
title = {{Coevolution of codon usage and transfer RNA abundance}},
volume = {325},
year = {1987}
}
@article{Page2013,
abstract = {Circulating nucleic acids (CNAs) are under investigation as a liquid biopsy in cancer. However there is wide variation in blood processing and methods for isolation of circulating free DNA (cfDNA) and microRNAs (miRNAs). Here we compare the extraction efficiency and reproducibility of 4 commercially available kits for cfDNA and 3 for miRNA using spike-in of reference templates. We also compare the effects of increasing time between venepuncture and centrifugation and differential centrifugation force on recovery of CNAs. cfDNA was quantified by TaqMan qPCR and targeted deep sequencing. miRNA profiles were assessed with TaqMan low-density arrays and assays. The QIAamp({\textregistered}) DNA Blood Mini and Circulating nucleic acid kits gave the highest recovery of cfDNA and efficient recovery ({\textgreater}90{\%}) of a 564bp spike-in. Moreover, targeted sequencing revealed overlapping cfDNA profiles and variant depth, including detection of HER2 gene amplification, using the Ion AmpliSeq™Cancer Hotspot Panel v2. Highest yields of miRNA and the synthetic Arabidopsis thaliana miR-159a spike-in were obtained using the miRNeasy Serum/Plasma kit, with saturation above 200 µl of plasma. miRNA profiles showed significant variation with increasing time before centrifugation (p{\textless}0.001) and increasing centrifugation force, with depletion of platelet associated miRNAs, whereas cfDNA was unaffected. However, sample replicates showed excellent reproducibility on TaqMan low density arrays ($\rho$ = 0.96, p{\textless}0.0001). We also successfully generated miRNA profiles for plasma samples stored {\textgreater} 12 years, highlighting the potential for analysis of stored sample biobanks. In the era of the liquid biopsy, standardisation of methods is required to minimise variation, particularly for miRNA.},
author = {Page, Karen and Guttery, David S. and Zahra, Nathalie and Primrose, Lindsay and Elshaw, Shona R. and Pringle, J. Howard and Blighe, Kevin and Marchese, Stephanie D. and Hills, Allison and Woodley, Laura and Stebbing, Justin and Coombes, R. Charles and Shaw, Jacqueline A.},
doi = {10.1371/journal.pone.0077963},
editor = {Otsuka, Motoyuki},
file = {:home/richardjamesacton/Documents/PhD{\_}Stuff/Papers/mendeleyPapers/Page et al.{\_}2013{\_}PloS one{\_}Influence of plasma processing on recovery and analysis of circulating nucleic acids.pdf:pdf},
isbn = {1932-6203 (Electronic) 1932-6203 (Linking)},
issn = {1932-6203},
journal = {PloS one},
month = {oct},
number = {10},
pages = {e77963},
pmid = {24205045},
title = {{Influence of plasma processing on recovery and analysis of circulating nucleic acids.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24205045},
volume = {8},
year = {2013}
}
@article{VanIterson2017,
abstract = {We show that epigenome- and transcriptome-wide association studies (EWAS and TWAS) are prone to significant inflation and bias of test statistics, an unrecognized phenomenon introducing spurious findings if left unaddressed. Neither GWAS-based methodology nor state-of-the-art confounder adjustment methods completely remove bias and inflation. We propose a Bayesian method to control bias and inflation in EWAS and TWAS based on estimation of the empirical null distribution. Using simulations and real data, we demonstrate that our method maximizes power while properly controlling the false positive rate. We illustrate the utility of our method in large-scale EWAS and TWAS meta-analyses of age and smoking.},
author = {van Iterson, Maarten and van Zwet, Erik W. and Heijmans, Bastiaan T.},
