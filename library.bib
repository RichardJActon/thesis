@article{Stagi2016,
abstract = {Peripheral quantitative computed tomography provides an automatical scan analysis of trabecular and cortical bone compartments, calculating not only their bone mineral density (BMD), but also bone geometrical parameters, such as marrow and cortical Cross-Sectional Area (CSA), Cortical Thickness (CoTh), both periosteal and endosteal circumference, as well as biomechanical parameters like Cross-Sectional Moment of Inertia (CSMI), a measure of bending, polar moment of inertia, indicating bone strength in torsion, and Strength Strain Index (SSI). Also CSA of muscle and fat can be extracted. Muscles, which are thought to stimulate bones to adapt their geometry and mineral content, are determinant to preserve or increase bone strength; thus, pQCT provides an evaluation of the functional 'muscle-bone unit', defined as BMC/muscle CSA ratio. This functional approach to bone densitometry can establish if bone strength is normally adapted to the muscle force, and if muscle force is adequate for body size, providing more detailed insights to targeted strategies for the prevention and treatment of bone fragility. The present paper offers an extensive review of technical features of pQCT and its possible clinical application in the diagnostic of bone status as well as in the monitoring of the skeleton's health follow-up.},
author = {Stagi, Stefano and Cavalli, Loredana and Cavalli, Tiziana and {De Martino}, Maurizio and Brandi, Maria Luisa},
doi = {10.1186/s13052-016-0297-9},
file = {:home/richardjamesacton/Documents/PhD{\_}Stuff/Papers/Stagi2016{\_}ItalianJpediatrics({\_}{\_}REVIEW-pQCT-bone-development{\_}{\_}).pdf:pdf},
issn = {18247288},
journal = {Italian Journal of Pediatrics},
keywords = {Bone mass,Bone mineral density,Bone mineralization,Fat area,Muscle area,Osteoporosis,Peripheral quantitative computed tomography (pQCT)},
number = {1},
pages = {1--20},
publisher = {Italian Journal of Pediatrics},
title = {{Peripheral quantitative computed tomography (pQCT) for the assessment of bone strength in most of bone affecting conditions in developmental age: A review}},
url = {http://dx.doi.org/10.1186/s13052-016-0297-9},
volume = {42},
year = {2016}
}
@article{Orioli2017,
abstract = {Recent years have seen a burst in the number of studies investigating tRNA biology. With the transition from a gene-centred to a genome-centred perspective, tRNAs and other RNA polymerase III transcripts surfaced as active regulators of normal cell physiology and disease. Novel strategies removing some of the hurdles that prevent quantitative tRNA profiling revealed that the differential exploitation of the tRNA pool critically affects the ability of the cell to balance protein homeostasis during normal and stress conditions. Furthermore, growing evidence indicates that the adaptation of tRNA synthesis to cellular dynamics can influence translation and mRNA stability to drive carcinogenesis and other pathological disorders. This review explores the contribution given by genomics, transcriptomics and epitranscriptomics to the discovery of emerging tRNA functions, and gives insights into some of the technical challenges that still limit our understanding of the RNA polymerase III transcriptional machinery.},
author = {Orioli, Andrea},
doi = {10.1002/bies.201600158},
file = {:home/richardjamesacton/Documents/PhD{\_}Stuff/Papers/mendeleyPapers/Orioli{\_}2017{\_}BioEssays news and reviews in molecular, cellular and developmental biology{\_}tRNA biology in the omics era Stress signalling.pdf:pdf},
issn = {1521-1878},
journal = {BioEssays : news and reviews in molecular, cellular and developmental biology},
keywords = {RNA polymerase III,epitranscriptomics,genomics,high-throughput,tRNA,transcriptomics},
month = {mar},
number = {3},
pages = {1600158},
pmid = {28026052},
title = {{tRNA biology in the omics era: Stress signalling dynamics and cancer progression.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/28026052},
volume = {39},
year = {2017}
}
@article{Moon2016,
abstract = {Context: Current approaches to antenatal vitamin D supplementation do not account for inter-individual differences in 25-hydroxyvitamin D (25(OH)D) response. Objective: We assessed which maternal and environmental characteristics were associated with 25(OH)D after supplementation with cholecalciferol. Design: Within-randomization-group analysis of participants in the Maternal Vitamin D Osteo-porosis Study trial of vitamin D supplementation in pregnancy. Setting: Hospital antenatal clinics. Participants: A total of 829 pregnant women (422 placebo, 407 cholecalciferol). At 14 and 34 weeks of gestation, maternal anthropometry, health, and lifestyle were assessed and 25(OH)D measured. Compliance was determined using pill counts at 19 and 34 weeks. Interventions: 1000 IU/d of cholecalciferol or matched placebo from 14 weeks of gestation until delivery. Main Outcome Measure: 25(OH)D at 34 weeks, measured in a single batch (Diasorin Liaison). Results: 25(OH)D at 34 weeks of gestation was higher in the women randomized to vitamin D (mean [SD], 67.7 [21.3] nmol/L) compared with placebo (43.1 [22.5] nmol/L; P Ͻ .001). In women randomized to cholecalciferol, higher pregnancy weight gain from 14 to 34 weeks of gestation (kg) (␤ ϭ Ϫ0.81 [95{\%} confidence interval Ϫ1.39, Ϫ0.22]), lower compliance with study medication ({\%}) (␤ ϭ Ϫ0.28 [Ϫ0.072, Ϫ0.48]), lower early pregnancy 25(OH)D (nmol/L) (␤ ϭ 0.28 [0.16, 0.40]), and delivery in the winter vs the summer (␤ ϭ Ϫ10.5 [Ϫ6.4, Ϫ14.6]) were independently associated with lower 25(OH)D at 34 weeks of gestation. Conclusions: Women who gained more weight during pregnancy had lower 25(OH)D in early preg-nancy and delivered in winter achieved a lower 25(OH)D in late pregnancy when supplemented with 1000 IU/d cholecalciferol. Future studies should aim to determine appropriate doses to enable consis-tent repletion of 25(OH)D during pregnancy. (J Clin Endocrinol Metab 101: 0000 – 0000, 2016)},
author = {Moon, Rebecca J. and Harvey, Nicholas C. and Cooper, Cyrus and D'Angelo, Stefania and Crozier, Sarah R. and Inskip, Hazel M. and Schoenmakers, Inez and Prentice, Ann and Arden, Nigel K. and Bishop, Nicholas J. and Carr, Andrew and Dennison, Elaine M. and Eastell, Richard and Fraser, Robert and Gandhi, Saurabh V. and Godfrey, Keith M. and Kennedy, Stephen and Mughal, M. Zulf and Papageorghiou, Aris T. and Reid, David M. and Robinson, Sian M. and Javaid, M. Kassim},
doi = {10.1210/jc.2016-2869},
