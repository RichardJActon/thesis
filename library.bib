isbn = {2213-8595},
issn = {22138587},
journal = {The Lancet Diabetes {\&} Endocrinology},
month = {may},
number = {5},
pages = {393--402},
pmid = {26944421},
publisher = {Cooper et al. Open Access article distributed under the terms of CC BY},
title = {{Maternal gestational vitamin D supplementation and offspring bone health (MAVIDOS): a multicentre, double-blind, randomised placebo-controlled trial}},
