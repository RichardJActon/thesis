journal = {BMC genomics},
keywords = {CpG Islands,DNA,DNA Methylation,Gene Deletion,Genome,Genotype,High-Throughput Nucleotide Sequencing,Human,Humans,Insertional,Mutagenesis,Oligonucleotide Array Sequence Analysis,Polymorphism,Protein Interaction Maps,Sequence Analysis,Single Nucleotide},
month = {jan},
