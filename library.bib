journal = {Oncogene},
keywords = {Animals,Apoptosis,COS Cells,Catalysis,Cell Line,Cercopithecus aethiops,Cyclin-Dependent Kinase Inhibitor p21,Cyclin-Dependent Kinase Inhibitor p21: genetics,Doxorubicin,Doxorubicin: pharmacology,Genetic,Humans,Nerve Tissue Proteins,Nerve Tissue Proteins: physiology,Promoter Regions,RNA,Small Interfering,Small Interfering: pharmacology,Transcription,Tumor,Tumor Suppressor Protein p53,Tumor Suppressor Protein p53: physiology,Ubiquitin-Protein Ligases,Ubiquitin-Protein Ligases: physiology,bcl-2-Associated X Protein,bcl-2-Associated X Protein: genetics},
