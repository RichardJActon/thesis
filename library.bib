issn = {0950382X},
journal = {Molecular Microbiology},
keywords = {Bacterial,Bacterial Proteins,Bacterial Proteins: genetics,Bacterial Proteins: metabolism,Gene Expression Regulation,Movement,Mutagenesis,Myxococcus xanthus,Myxococcus xanthus: cytology,Myxococcus xanthus: genetics,Myxococcus xanthus: metabolism,Protein Interaction Domains and Motifs},
