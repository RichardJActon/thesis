issn = {1474-9726},
journal = {Aging cell},
keywords = {Aging,Biomarker,DNA methylation,ELOVL 2,FHL 2,PENK},
month = {dec},
number = {6},
pages = {1132--4},
pmid = {23061750},
title = {{Methylation of ELOVL2 gene as a new epigenetic marker of age.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23061750},
volume = {11},
