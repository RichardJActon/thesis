pages = {1842--1855.e16},
pmid = {30449618},
publisher = {Elsevier Inc.},
title = {{Transcription Factors Activate Genes through the Phase-Separation Capacity of Their Activation Domains}},
url = {https://linkinghub.elsevier.com/retrieve/pii/S0092867418313989},
volume = {175},
