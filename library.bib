@article{Johansen1976,
abstract = {Respiratory properties of whole blood and Hb solutions have been studied in Heterocephalus glaber, a fossorial rodent, having a low body temperature (30.0–32.0 C) and poor thermoregulatory ability. For comparison similar, measurements were made on laboratory mice. Mus musculus. Whole blood showed a distinctly higher O2 affinity for Heterocephalus at both 30 and 37°C. P50 values were 23.3 mm Hg and 33.0mm H gat 37°C for Heterocephalus and Mus, respectively, while at 30°C P50's were 18.8mm Hg and 24.9 mm Hg, all values at pH (b) 7.4. $\Delta$H values (expressive of the effect of temperature on P50) were −5.8 kcal {\textperiodcentered} mol−1 for Heterocephalus and −7.5 kcal {\textperiodcentered} mol−1 for Mus. The CO2 Bohr effects ($\phi$) were −0.43 and −0.50 for Heterocephalus at 37 and 30°C. Corresponding values for Mus were −0.65 and −0.56. Both species had a Hill's n-value of 2.6. Red cell concentrations of 2,3-DGP were closely similar in the species being 7.3 mmol {\textperiodcentered} L− rbc for Heterocephalus and 7.4 mmol {\textperiodcentered} L− rbe for Mus. Stripped Heterocephalus Hb had a very high O2 affinity, at pH 7.25. 37°C, P50 was 8.0 mm Hg whereas the corresponding value for Mus was 11.3 mm Hg. Addition of DPG to stripped Hb from the two species decreased O2 affinity to the same degree. The high 02 affinity of Heterocephalus blood is viewed as a possible adaptation to its burrowing habits. Its basis is inherent to the hemoglobin molecule itself and not dependent upon cofactor influence or the temperature sensitivity of the O2-Hb binding.},
author = {Johansen, Kjell and Lykkeboe, Gunnar and Weber, Roy E. and Maloiy, G.M.O.},
doi = {10.1016/0034-5687(76)90025-6},
issn = {00345687},
