file = {:home/richardjamesacton/Documents/PhD{\_}Stuff/Papers/mendeleyPapers/Zhang et al.{\_}2015{\_}Genome biology{\_}Predicting genome-wide DNA methylation using methylation marks, genomic position, and DNA regulatory el.pdf:pdf},
isbn = {1465-6906},
issn = {1474-760X},
journal = {Genome biology},
month = {jan},
number = {1},
pages = {14},
pmid = {25616342},
title = {{Predicting genome-wide DNA methylation using methylation marks, genomic position, and DNA regulatory elements.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/25616342},
