journal = {Cell},
keywords = {Aging,Animals,Autophagy,Cell Death,Homeostasis,Humans,Mammals,Mammals: physiology,Mitochondria,Mitochondria: physiology,Phagosomes,Phagosomes: physiology,Yeasts,Yeasts: cytology},
month = {sep},
