issn = {1098-5549},
journal = {Molecular and cellular biology},
keywords = {SMC proteins,Saccharomyces cerevisiae,chromatin,chromosome structure,gene silencing,nucleosome,tDNA},
month = {feb},
number = {8},
pages = {1--26},
pmid = {30718362},
title = {{tRNA Genes Affect Chromosome Structure and Function via Local Effects.}},
