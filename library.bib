doi = {10.1128/JB.01942-06},
file = {:home/richardjamesacton/Documents/PhD{\_}Stuff/Papers/mendeleyPapers/Evans, Lambert, Sockett{\_}2007{\_}Journal of Bacteriology{\_}Predation by Bdellovibrio bacteriovorus HD100 Requires Type IV Pili.pdf:pdf},
