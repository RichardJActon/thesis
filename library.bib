isbn = {1465-6914 (Electronic)$\backslash$r1465-6906 (Linking)},
issn = {1474-760X},
journal = {Genome biology},
keywords = {Cell Line,DNA,DNA Methylation,DNA: methods,Female,Gene Expression Profiling,Gene Expression Regulation,Genetic,Genome,Human,Humans,Male,Models,Molecular Sequence Data,Promoter Regions,RNA,Sequence Analysis,Statistical},
