file = {:home/richardjamesacton/Documents/PhD{\_}Stuff/Papers/mendeleyPapers/Loewen et al.{\_}1998{\_}Canadian journal of microbiology{\_}Regulation in the rpoS regulon of Escherichia coli.pdf:pdf},
