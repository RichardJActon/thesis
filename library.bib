isbn = {1305901813971},
issn = {1474-760X},
journal = {Genome Biology},
keywords = {Ageing,Epigenetics,Methylation},
month = {dec},
number = {1},
pages = {22},
pmid = {29452591},
publisher = {Genome Biology},
title = {{Ageing-associated DNA methylation dynamics are a molecular readout of lifespan variation among mammalian species}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/29452591},
volume = {19},
