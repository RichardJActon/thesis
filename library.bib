pages = {e323--e323},
pmid = {28450733},
publisher = {Nature Publishing Group},
title = {{TET family dioxygenases and DNA demethylation in stem cells and cancers}},
url = {http://dx.doi.org/10.1038/emm.2017.5},
volume = {49},
year = {2017}
}
