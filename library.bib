issn = {1535-9476},
journal = {Molecular {\&} Cellular Proteomics},
keywords = {Antibodies,Cell Compartmentation,Cell Line,Confocal,Humans,Internet,Microscopy,Neoplasm,Protein Transport,Proteome,Proteome: metabolism,Subcellular Fractions,Tumor},
month = {mar},
number = {3},
pages = {499--508},
pmid = {18029348},
title = {{Toward a Confocal Subcellular Atlas of the Human Proteome}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/18029348},
volume = {7},
