file = {:home/richardjamesacton/Documents/PhD{\_}Stuff/Papers/mendeleyPapers/Amikam, Galperin{\_}2006{\_}Bioinformatics{\_}PilZ domain is part of the bacterial c-di-GMP binding protein.pdf:pdf},
