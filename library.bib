@article{Henderson1999,
abstract = {The expression of most bacterial genes is controlled at the level of transcription via promoter control mechanisms that permit a graded response. However, an increasing number of bacterial genes are found to exhibit an 'all-or-none' control mechanism that adapts the bacterium to more than one environment. One such mechanism is phase variation, traditionally defined as the high-frequency ON{\textless}--{\textgreater}OFF switching of phenotype expression. Phase variation events are usually random, but may be modulated by environmental conditions. The mechanisms of phase variation events and their significance within the microbial community are discussed here.},
author = {Henderson, I R and Owen, P and Nataro, J P},
file = {:home/richardjamesacton/Documents/PhD{\_}Stuff/Papers/mendeleyPapers/Henderson, Owen, Nataro{\_}1999{\_}Molecular microbiology{\_}Molecular switches--the ON and OFF of bacterial phase variation.pdf:pdf},
