issn = {1945-4589},
journal = {Aging},
keywords = {Age,Dna methylation,Epigenetic,Predictor,Tissue},
month = {oct},
number = {10},
pages = {1018--27},
pmid = {22067257},
title = {{Epigenetic-aging-signature to determine age in different tissues.}},
