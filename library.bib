journal = {Nucleic acids research},
month = {jan},
number = {Database issue},
pages = {D93--7},
pmid = {18984615},
title = {{GtRNAdb: a database of transfer RNA genes detected in genomic sequence.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/18984615},
volume = {37},
year = {2009}
}
@article{Lo1998a,
abstract = {We have developed a real-time quantitative PCR assay to measure the concentration of fetal DNA in maternal plasma and serum. Our results show that fetal DNA is present in high concentrations in maternal plasma, reaching a mean of 25.4 genome equivalents/ml (range 3.3-69. 4) in early pregnancy and 292.2 genome equivalents/ml (range 76. 9-769) in late pregnancy. These concentrations correspond to 3.4{\%} (range 0.39{\%}-11.9{\%}) and 6.2{\%} (range 2.33{\%}-11.4{\%}) of the total plasma DNA in early and late pregnancy, respectively. Sequential follow-up study of women who conceived by in vitro fertilization shows that fetal DNA can be detected in maternal serum as early as the 7th wk of gestation and that it then increases in concentration as pregnancy progresses. These data suggest that fetal DNA can be readily detected in maternal plasma and serum and may be a valuable source of material for noninvasive prenatal diagnosis.},
