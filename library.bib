month = {aug},
number = {8},
pages = {e1000602},
pmid = {19680444},
title = {{Aging and Environmental Exposures Alter Tissue-Specific DNA Methylation Dependent upon CpG Island Context}},
url = {https://dx.plos.org/10.1371/journal.pgen.1000602},
volume = {5},
year = {2009}
}
