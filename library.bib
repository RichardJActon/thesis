journal = {Protein science : a publication of the Protein Society},
keywords = {Amino Acid Sequence,Base Sequence,Chemistry,Chromatography,Circular Dichroism,Cysteine,Cysteine: analysis,DNA,DNA Footprinting,DNA-Binding Proteins,DNA-Binding Proteins: chemistry,DNA-Binding Proteins: metabolism,Deoxyribonuclease I,Fungal,Fungal Proteins,Fungal Proteins: chemistry,Fungal Proteins: metabolism,Fungal: chemistry,Fungal: metabolism,Gel,Magnetic Resonance Spectroscopy,Meiosis,Meiosis: genetics,Molecular Sequence Data,Nucleic Acid Conformation,Physical,Physicochemical Phenomena,Protein Conformation,Repressor Proteins,Saccharomyces cerevisiae,Saccharomyces cerevisiae Proteins,Saccharomyces cerevisiae: chemistry,Saccharomyces cerevisiae: genetics,Transcription Factors,Transcription Factors: chemistry,Transcription Factors: metabolism,Zinc,Zinc: analysis,Zinc: pharmacology,atg8,ume6},
