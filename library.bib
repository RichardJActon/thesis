journal = {Nature Cell Biology},
keywords = {Adenosine,Adenosine: analogs {\&} derivatives,Adenosine: metabolism,Amino Acid,Amino Acid Sequence,Animals,Cell Line,Developmental,Embryonic Stem Cells,Embryonic Stem Cells: cytology,Embryonic Stem Cells: metabolism,Gene Expression Regulation,Gene Knockdown Techniques,Humans,Messenger,Messenger: genetics,Methyltransferases,Methyltransferases: chemistry,Methyltransferases: genetics,Mice,Molecular Sequence Data,RNA,Sequence Homology},
