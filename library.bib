journal = {The Journal of biological chemistry},
keywords = {Amino Acid,Amino Acid Sequence,Autophagy,Catalytic Domain,Crystallography,Glutathione Transferase,Glutathione Transferase: metabolism,Microtubule-Associated Proteins,Microtubule-Associated Proteins: chemistry,Models,Molecular,Molecular Sequence Data,Protein Binding,Protein Conformation,Protein Structure,Saccharomyces cerevisiae,Saccharomyces cerevisiae Proteins,Saccharomyces cerevisiae Proteins: chemistry,Saccharomyces cerevisiae Proteins: metabolism,Saccharomyces cerevisiae: metabolism,Secondary,Sequence Homology,Ubiquitin-Conjugating Enzymes,Ubiquitin-Conjugating Enzymes: chemistry,X-Ray},
