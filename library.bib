keywords = {Bacteria,Bacteria: pathogenicity,Bacterial Proteins,Bacterial Proteins: genetics,Bacterial Proteins: metabolism,Drug Discovery,Saccharomyces cerevisiae,Saccharomyces cerevisiae: genetics,Saccharomyces cerevisiae: metabolism,Virulence Factors,Virulence Factors: genetics,Virulence Factors: metabolism},
