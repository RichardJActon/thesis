pages = {3983--3989},
title = {{Meffil: efficient normalization and analysis of very large DNA methylation datasets}},
url = {https://academic.oup.com/bioinformatics/advance-article/doi/10.1093/bioinformatics/bty476/5042224},
volume = {34},
