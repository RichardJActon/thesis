file = {:home/richardjamesacton/Documents/PhD{\_}Stuff/Papers/mendeleyPapers/Yoshihisa{\_}2014{\_}Frontiers in Genetics{\_}Handling tRNA introns, archaeal way and eukaryotic way.pdf:pdf},
