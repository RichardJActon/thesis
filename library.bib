issn = {0950-382X},
journal = {Molecular microbiology},
keywords = {Amino Acid,Amino Acid Sequence,Bacterial,Bacterial Outer Membrane Proteins,Bacterial Proteins,Bacterial Proteins: chemistry,Bacterial Proteins: genetics,Bacterial Proteins: metabolism,Cell Membrane,Cell Membrane: metabolism,Consensus Sequence,Escherichia coli,Escherichia coli Proteins,Escherichia coli: genetics,Escherichia coli: growth {\&} development,Escherichia coli: metabolism,Gene Expression Regulation,Ion Transport,Lipoproteins,Lipoproteins: genetics,Lipoproteins: metabolism,Membrane Proteins,Molecular Sequence Data,Mutation,Peptidoglycan,Peptidoglycan: genetics,Peptidoglycan: metabolism,Proteoglycans,Sequence Homology},
month = {nov},
number = {3},
