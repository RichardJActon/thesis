journal = {BioEssays : news and reviews in molecular, cellular and developmental biology},
keywords = {Bacillus,Bacillus: genetics,Bacillus: physiology,Bacterial Physiological Phenomena,Biological,Codon,Escherichia coli,Escherichia coli: metabolism,Genetic,Models,Open Reading Frames,Protein Biosynthesis,RNA,RNA: metabolism,Transcription,Transfer,Transfer: metabolism},
month = {aug},
number = {8},
pages = {700--7},
pmid = {12210530},
title = {{Regulation by transcription attenuation in bacteria: how RNA provides instructions for transcription termination/antitermination decisions.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/12210530},
volume = {24},
year = {2002}
}
