issn = {1474-760X},
journal = {Genome biology},
keywords = {80 and over,Adolescent,Adult,Aged,Aging,Brain,Child,CpG Islands,DNA Methylation,Databases,Disease,Dna,Epigenesis,Epigenomics,Female,Genes,Genetic,Humans,Infant,Male,Methylation,Middle Aged,Nervous System,Preschool,Young Adult,analysis,blood,genetics,metabolism,methods},
