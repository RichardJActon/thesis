@article{Nilforoushan2009a,
abstract = {Osteoclasts are multinucleated bone resorbing cells which form by fusion of pre-osteoclasts. Here, we investigate how nitric oxide (NO) affects osteoclastogenesis. Time lapse photomicrography, using the fluorescent NO indicator dye, 4,5-diaminofluorescein diacetate, revealed an intense NO signal in pre-osteoclasts preceding cell fusion. Osteoclastogenesis in RAW264.7 cells increased when exposed to the NO synthase inhibitor, l-NMMA (0.25 $\mu$M), for the initial 48 h. In contrast, pre-osteoclast fusion decreased when RAW264.7 cells were exposed to l-NMMA from 48 to 96 h. Both NO synthase inhibitors, l-NMMA and l-NAME, decreased osteoclast formation during this time period. The inhibitory effect of l-NMMA on osteoclast formation was abolished with increasing concentrations (25-200 ng/ml) of sRANKL suggesting signaling cross talk. NO donors increased osteoclast formation in a dose-dependent manner, with greatest stimulation at 15 $\mu$M NOC-12 (2.3 fold) and 5 $\mu$M NOC-18 (2.4 fold). Measuring nitrite (NO end product) daily from culture media of RAW264.7 cells undergoing osteoclastogenesis revealed that an increase in NO production coincided with the fusion of pre-osteoclasts (day 4). Inhibiting fusion by plating cells on polystyrene dishes pre-coated with poly-(l-lysine) decreased both osteoclast formation and NO production. To address if NO mediates fusion through the actin cytoskeleton, actin free barbed ends were measured. 0.25 $\mu$M l-NMMA decreased, while 15 $\mu$M NOC-12 and 5 $\mu$M NOC-18 increased actin free barbed ends. We hypothesize that while NO initially negatively regulates pre-osteoclast differentiation; it later facilitates the fusion of mononuclear pre-osteoclasts, possibly by up regulating actin remodeling. {\textcopyright} 2009.},
author = {Nilforoushan, Dorrin and Gramoun, Azza and Glogauer, Michael and Manolson, Morris F.},
doi = {10.1016/j.niox.2009.04.002},
file = {:home/richardjamesacton/Documents/PhD{\_}Stuff/Papers/mendeleyPapers/Nilforoushan et al.{\_}2009{\_}Nitric Oxide{\_}Nitric oxide enhances osteoclastogenesis possibly by mediating cell fusion.pdf:pdf},
isbn = {1089-8611 (Electronic)$\backslash$r1089-8603 (Linking)},
issn = {10898603},
journal = {Nitric Oxide},
keywords = {Fusion,Nitric oxide,Osteoclast,Osteoclastogenesis,Pre-osteoclast},
month = {aug},
number = {1},
pages = {27--36},
pmid = {19389479},
publisher = {Elsevier Inc.},
title = {{Nitric oxide enhances osteoclastogenesis possibly by mediating cell fusion}},
url = {http://dx.doi.org/10.1016/j.niox.2009.04.002},
volume = {21},
year = {2009}
}
