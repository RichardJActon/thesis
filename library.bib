journal = {Nature neuroscience},
keywords = {*Epigenomics,Animals,Brain/cytology/growth {\&} development/*metabolism,Chromatin Immunoprecipitation,CpG Islands/*physiology,DNA Methylation/*physiology,Electrophoretic Mobility Shift Assay,Embryo,Gene Expression/*genetics,Genome,Humans,Inbred C57BL,Male,Mammalian,Messenger/metabolism,Methyl-CpG-Binding Protein 2/genetics/metabolism,Mice,Nerve Tissue Proteins/genetics/metabolism,Neurons/*physiology,RNA},
