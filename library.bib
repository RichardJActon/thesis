journal = {Nature},
keywords = {Amino Acid Sequence,Biocatalysis,Catalytic Domain,Crystallography,DNA,DNA: chemistry,DNA: metabolism,Humans,Methylation,Models,Molecular,Molecular Sequence Data,Protein Binding,Protein Conformation,Proteins,Proteins: chemistry,Proteins: genetics,Proteins: metabolism,RNA,RNA: chemistry,RNA: metabolism,Single-Stranded,Single-Stranded: chemistry,Single-Stranded: metabolism,Structure-Activity Relationship,Substrate Specificity,Thymidine,Thymidine: analogs {\&} derivatives,Thymidine: chemistry,Thymidine: metabolism,Uracil,Uracil: analogs {\&} derivatives,Uracil: chemistry,Uracil: metabolism,X-Ray},
