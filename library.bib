month = {apr},
number = {13},
pages = {4862--4867},
pmid = {24639528},
title = {{Structures of the Toxoplasma gliding motility adhesin}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24639528},
volume = {111},
year = {2014}
}
