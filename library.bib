issn = {00225193},
journal = {Journal of Theoretical Biology},
keywords = {Division of labour,Evolution of multicellularity,Evolutionary game theory,Phenotypic plasticity,Social dilemmas},
month = {apr},
pages = {161--173},
pmid = {26869214},
publisher = {Elsevier},
title = {{Game theoretic treatments for the differentiation of functional roles in the transition to multicellularity}},
url = {http://dx.doi.org/10.1016/j.jtbi.2016.01.041},
volume = {395},
year = {2016}
}
