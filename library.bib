file = {:home/richardjamesacton/Documents/PhD{\_}Stuff/Papers/mendeleyPapers/Maunakea et al.{\_}2010{\_}Nature{\_}Conserved role of intragenic DNA methylation in regulating alternative promoters.pdf:pdf;:home/richardjamesacton/Documents/PhD{\_}Stuff/Papers/mendeleyPapers/Maunakea et al.{\_}2010{\_}Nature{\_}Conserved role of intragenic DNA methylation in regulating alternative promoters(2).pdf:pdf},
