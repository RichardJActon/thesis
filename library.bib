file = {:home/richardjamesacton/Documents/PhD{\_}Stuff/Papers/mendeleyPapers/Chung, Baek{\_}1999{\_}Biochemical and biophysical research communications{\_}Deubiquitinating enzymes their diversity and emerging roles.pdf:pdf},
