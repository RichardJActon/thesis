isbn = {1546-1718 (Electronic)$\backslash$r1061-4036 (Linking)},
issn = {1546-1718},
journal = {Nature genetics},
month = {jul},
number = {7},
pages = {904--8},
pmid = {18568024},
title = {{Genomic surveys by methylation-sensitive SNP analysis identify sequence-dependent allele-specific DNA methylation.}},
