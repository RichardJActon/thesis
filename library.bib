keywords = {Bone regeneration,Bromodomain inhibitor,Genetics–Epigenetics,HDAC inhibitor,Osteoporosis},
month = {sep},
number = {9},
pages = {1446},
pmid = {27598138},
title = {{Epigenetic Regulation of Bone Remodeling and Its Impacts in Osteoporosis}},
