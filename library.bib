issn = {0021-9258},
journal = {The Journal of biological chemistry},
keywords = {Alanine,Alanine: chemistry,Amino Acid,Amino Acid Sequence,Binding Sites,Biological,Cell Line,Cell Membrane,Cell Membrane: metabolism,Cell-Free System,Culture Media,Cysteine,Cysteine Endopeptidases,Cysteine Endopeptidases: metabolism,Cysteine Endopeptidases: physiology,Cysteine: chemistry,Escherichia coli,Escherichia coli: metabolism,Fluorescence,GABA-A,GABA-A: chemistry,Genetic,Glutathione Transferase,Glutathione Transferase: metabolism,Green Fluorescent Proteins,HeLa Cells,Humans,Lipids,Lipids: chemistry,Luminescent Proteins,Luminescent Proteins: metabolism,Microscopy,Microtubule-Associated Proteins,Microtubule-Associated Proteins: chemistry,Microtubule-Associated Proteins: metabolism,Microtubules,Microtubules: metabolism,Models,Molecular Sequence Data,Mutagenesis,Mutation,Phagocytosis,Phospholipase D,Phospholipase D: chemistry,Plasmids,Plasmids: metabolism,Promoter Regions,Protein Binding,Protein Structure,RNA Interference,Receptors,Saccharomyces cerevisiae Proteins,Sequence Homology,Site-Directed,Tertiary,Transfection},
