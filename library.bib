journal = {Muscle {\&} Nerve},
keywords = {Databases,Duchenne,Duchenne: genetics,Gene Frequency,Genetic,Germany,Humans,Muscular Dystrophy,Mutation,Mutation: genetics,Phenotype,Reading Frames,Reading Frames: genetics},
month = {aug},
