pages = {816--21},
pmid = {22745249},
title = {{A programmable dual-RNA-guided DNA endonuclease in adaptive bacterial immunity.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22745249},
volume = {337},
year = {2012}
}
