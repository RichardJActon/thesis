issn = {0020-7136},
journal = {International Journal of Cancer},
keywords = {angiogenin,biomarker,colorectal cancer,metastasis,tiRNA},
month = {mar},
pages = {ijc.32245},
title = {{Angiogenin promotes colorectal cancer metastasis via tiRNA production}},
url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/ijc.32245},
year = {2019}
}
