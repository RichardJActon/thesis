file = {:home/richardjamesacton/Documents/PhD{\_}Stuff/Papers/mendeleyPapers/Bell et al.{\_}2012{\_}PLoS genetics{\_}Epigenome-wide scans identify differentially methylated regions for age and age-related phenotypes in a h.pdf:pdf},
isbn = {1553-7390},
