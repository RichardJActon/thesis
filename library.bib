issn = {1362-4962},
journal = {Nucleic acids research},
month = {may},
number = {8},
pages = {e63},
pmid = {16682442},
title = {{Characterization of RNase R-digested cellular RNA source that consists of lariat and circular RNAs from pre-mRNA splicing.}},
