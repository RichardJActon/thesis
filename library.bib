issn = {1545-2948},
journal = {Annual review of genetics},
keywords = {aminoacyl-trna,melas,merrf,mitochondrial disease,mitochondrial trna,synthetase,trna-modifying enzymes},
month = {dec},
number = {1},
pages = {299--329},
pmid = {21910628},
title = {{Human mitochondrial tRNAs: biogenesis, function, structural aspects, and diseases.}},
