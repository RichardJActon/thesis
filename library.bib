file = {:home/richardjamesacton/Documents/PhD{\_}Stuff/Papers/mendeleyPapers/Mallery et al.{\_}2010{\_}Proceedings of the National Academy of Sciences{\_}Antibodies mediate intracellular immunity through tripartite motif-c.pdf:pdf},
