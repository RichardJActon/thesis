title = {{Dynamic Alu Methylation during Normal Development, Aging, and Tumorigenesis}},
url = {http://www.hindawi.com/journals/bmri/2014/784706/},
volume = {2014},
year = {2014}
}
@article{Barker1986,
abstract = {Although the rise in ischaemic heart disease in England and Wales has been associated with increasing prosperity, mortality rates are highest in the least affluent areas. On division of the country into two hundred and twelve local authority areas a strong geographical relation was found between ischaemic heart disease mortality rates in 1968-78 and infant mortality in 1921-25. Of the twenty-four other common causes of death only bronchitis, stomach cancer, and rheumatic heart disease were similarly related to infant mortality. These diseases are associated with poor living conditions and mortality from them is declining. Ischaemic heart disease is strongly correlated with both neonatal and postneonatal mortality. It is suggested that poor nutrition in early life increases susceptibility to the effects of an affluent diet. {\textcopyright} 1986.},
