journal = {Proceedings of the National Academy of Sciences},
keywords = {Alleles,Animals,Drosophila Proteins,Drosophila Proteins: genetics,Drosophila Proteins: metabolism,Drosophila melanogaster,Female,Male,Mutation,Notch,Notch: genetics,Notch: metabolism,Nuclear Proteins,Nuclear Proteins: genetics,Nuclear Proteins: metabolism,Oogenesis,Oogenesis: physiology,Ovary,Ovary: metabolism,Receptors,Signal Transduction,Signal Transduction: physiology,Testis,Testis: metabolism},
