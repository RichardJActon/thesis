issn = {2050-084X},
journal = {eLife},
keywords = {OGT,TET,biochemistry,cell biology,chemical biology,chromatin,development,epigenetics,mouse,stem cells},
month = {oct},
pages = {1--23},
pmid = {30325306},
title = {{OGT binds a conserved C-terminal domain of TET1 to regulate TET1 activity and function in development}},
