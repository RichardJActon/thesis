issn = {09288244},
journal = {FEMS Immunology {\&} Medical Microbiology},
keywords = {Bacterial Proteins,Bacterial Proteins: metabolism,Cell Nucleus,Cell Nucleus: chemistry,Chlamydia trachomatis,Chlamydia trachomatis: pathogenicity,Cytoplasm,Cytoplasm: chemistry,Electrophoresis,Fluorescence,Gel,HeLa Cells,Humans,Immunoblotting,Microscopy,Protein Transport,Proteome,Proteome: analysis,Staining and Labeling,Staining and Labeling: methods,Sulfur Radioisotopes,Sulfur Radioisotopes: metabolism,Two-Dimensional,Virulence Factors,Virulence Factors: metabolism},
