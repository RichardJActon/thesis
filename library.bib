issn = {1547-6286},
journal = {RNA Biology},
keywords = {1000 genomes project,Anticodon,Base Pair Mismatch,Evolution,Genetic Variation,Genome,HeLa Cells,Human,Humans,Isodecoder,Molecular,Phylogeny,RNA,SNP,Transfer,Transfer RNA Aminoacylation,Transfer: chemistry,Transfer: genetics,Transfer: metabolism,tRNA},
month = {dec},
number = {12},
pages = {1853--1867},
pmid = {24448271},
title = {{Diversity of human tRNA genes from the 1000-genomes project}},
