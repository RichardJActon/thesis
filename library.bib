journal = {Genome Research},
keywords = {Bacterial,Bacterial: chemistry,DNA,DNA: methods,Escherichia coli,Escherichia coli: chemistry,Guanosine,Guanosine: analogs {\&} derivatives,Guanosine: chemistry,Humans,Kinetics,Mitochondrial,Mitochondrial: chemistry,Oxidation-Reduction,Sequence Analysis},
