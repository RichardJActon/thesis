doi = {10.1093/nar/gkq1027},
file = {:home/richardjamesacton/Documents/PhD{\_}Stuff/Papers/Kozomara2011{\_}NAR({\_}{\_}mirBase-microRNA-database{\_}{\_}).pdf:pdf},
