issn = {14394227},
journal = {ChemBioChem},
keywords = {Biotin,Biotin: chemistry,Biotinylation,Catalytic Domain,Cell Line,Endopeptidases,Endopeptidases: metabolism,Fluorescent Dyes,Fluorescent Dyes: chemical synthesis,Fluorescent Dyes: chemistry,Fluorescent Dyes: metabolism,Humans,Solid-Phase Synthesis Techniques,Tumor,Ubiquitin,Ubiquitin: chemistry,Ubiquitin: metabolism,Ubiquitination},
