journal = {Journal of biochemistry},
keywords = {Adenosine Triphosphatases,Adenosine Triphosphatases: genetics,Bacterial,Bacterial Proteins,Bacterial Proteins: genetics,Bacterial: genetics,Cation Transport Proteins,Cation Transport Proteins: genetics,Escherichia coli,Escherichia coli Proteins,Escherichia coli Proteins: genetics,Escherichia coli: genetics,Gene Expression Regulation,Genetic,Mutation,Promoter Regions,Signal Transduction,Signal Transduction: genetics,Trans-Activators,Trans-Activators: genetics,Transcription},
month = {jan},
number = {1},
pages = {51--9},
pmid = {15713883},
title = {{Molecular basis for promoter selectivity of the transcriptional activator OmpR of Escherichia coli: isolation of mutants that can activate the non-cognate kdpABC promoter.}},
