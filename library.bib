isbn = {1044071060},
issn = {0305-1048},
journal = {Nucleic Acids Research},
month = {may},
number = {9},
pages = {5142--5152},
title = {{tRNA-derived small RNAs target transposable element transcripts}},
url = {https://academic.oup.com/nar/article-lookup/doi/10.1093/nar/gkx103},
volume = {45},
year = {2017}
}
