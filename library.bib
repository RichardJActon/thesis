journal = {Journal of Bacteriology},
keywords = {Bacterial,Bacterial Proteins,Bacterial Proteins: genetics,Bacterial Proteins: metabolism,Bacterial: physiology,Chromosome Mapping,Computational Biology,Gene Expression Regulation,Microbial Interactions,Microbial Interactions: physiology,Movement,Mutation,Myxococcus xanthus,Myxococcus xanthus: genetics,Myxococcus xanthus: metabolism,Protein Conformation,Protein Structure,Tertiary},
