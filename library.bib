journal = {Science Advances},
month = {nov},
number = {11},
pages = {eaao1617},
pmid = {29152572},
title = {{A null mutation in SERPINE1 protects against biological aging in humans}},
url = {http://advances.sciencemag.org/lookup/doi/10.1126/sciadv.aao1617},
volume = {3},
year = {2017}
}
