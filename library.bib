isbn = {1474-760X (Electronic)$\backslash$r1474-7596 (Linking)},
issn = {1474-760X},
journal = {Genome Biology},
keywords = {Adipose Tissue,Adipose Tissue: metabolism,Blood,Blood: metabolism,CpG Islands,DNA Methylation,Dizygotic,Female,Gene-Environment Interaction,Genetic Variation,Genome,Human,Humans,Monozygotic,Smoking,Smoking: genetics,Twins},
month = {dec},
number = {1},
