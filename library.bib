file = {:home/richardjamesacton/Documents/PhD{\_}Stuff/Papers/Martin-Trujillo2017{\_}Ncomms({\_}{\_}copy-number-variation-methylation-cancer{\_}{\_}).pdf:pdf},
issn = {2041-1723},
journal = {Nature Communications},
month = {dec},
number = {1},
pages = {467},
pmid = {28883545},
title = {{Copy number rather than epigenetic alterations are the major dictator of imprinted methylation in tumors}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/28883545},
volume = {8},
year = {2017}
}
