file = {:home/richardjamesacton/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Avelar et al. - 2020 - A multidimensional systems biology analysis of cellular senescence in aging and disease.pdf:pdf},
issn = {1474-760X},
