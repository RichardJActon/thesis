issn = {1474-760X},
journal = {Genome biology},
keywords = {Algorithms,CpG Islands,CpG Islands: genetics,DNA,DNA Methylation,DNA Methylation: genetics,DNA: methods,High-Throughput Nucleotide Sequencing,High-Throughput Nucleotide Sequencing: methods,Humans,Models,Sequence Analysis,Software,Statistical},
